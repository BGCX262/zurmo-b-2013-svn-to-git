$('#MassEdit_employees').click(function()
    {
        if (this.checked)
        {
            $('#Account_employees').removeAttr('disabled'); 
if ($('#Account_employees').attr('type') != 'button')
                    {
                        if ($('#Account_employees').attr('href') != undefined)
                        {
                            $('#Account_employees').css('display', '');
                        }
                    }; 

        }
        else
        {
            $('#Account_employees').attr('disabled', 'disabled'); 
if ($('#Account_employees').attr('type') != 'button')
                    {
                        if ($('#Account_employees').attr('href') != undefined)
                        {
                            $('#Account_employees').css('display', 'none');
                        }
                        $('#Account_employees').val('');
                    }; 

        }
    }
);
});
