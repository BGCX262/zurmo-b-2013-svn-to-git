                                var dropDown            = $(this);
                                var notificationBarId   = "FlashMessageBar";
                                var url                 = "/zurmo/app/index.php/emailTemplates/default/details";
                                var disableDropDown     = "1";
                                var disableTextBox      = "1";
                                var textContentId       = "Campaign_textContent";
